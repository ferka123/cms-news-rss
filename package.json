{
  "name": "cms-news-rss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local-dev": "docker-compose -f docker-compose.dev.yml up db minio -d && npx prisma generate && npm-run-all -p web-dev importer-dev",
    "web-dev": "next dev",
    "importer-dev": "nodemon --watch rss-importer --legacy-watch -P 1000 rss-importer/src/server.ts",
